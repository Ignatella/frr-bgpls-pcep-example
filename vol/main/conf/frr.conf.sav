frr version 9.0.1_git
frr defaults traditional
hostname main
log file /frr.log
no ipv6 forwarding
!
interface eth1
 ip router isis 1
 mpls enable
exit
!
interface eth2
 ip router isis 1
 mpls enable
exit
!
interface lo
 ip address 172.21.0.1/32
 ip router isis 1
 mpls enable
exit
!
router isis 1
 net 49.0001.1111.1111.1111.00
 segment-routing on
 segment-routing node-msd 8
 segment-routing prefix 172.21.0.1/32 index 1 no-php-flag
exit
!
